<body class="bg-white text-center">

  <!-- Reviews Modal Backdrop -->
  <div id="reviewsModalBackdrop" class="hidden fixed inset-0 bg-black/40 z-40"></div>

  <!-- Reviews Modal -->
<div id="reviewsModal" class="hidden fixed inset-0 z-50 flex items-center justify-center">
  <div class="bg-white rounded-lg w-full max-w-xl shadow-lg p-6 relative flex flex-col max-h-[80vh] overflow-hidden">
    
    <button id="closeReviewsModal" class="absolute top-3 right-4 text-gray-400 hover:text-black text-xl">&times;</button>
    
    <h2 class="text-lg font-semibold mb-4 text-center">Customer Reviews</h2>

    <!-- Review List -->
    <div id="reviewsContainer" class="flex flex-col gap-4 overflow-y-auto pr-1 mb-4 max-h-[40vh]">
      <!-- Reviews will be injected here -->
    </div>

    <!-- New Review Form -->
    <form id="reviewForm" class="flex flex-col gap-3 border-t pt-4">
      <input
        type="text"
        id="reviewUser"
        placeholder="Your name"
        class="border rounded px-3 py-2 text-sm"
        required
      />

      <textarea
        id="reviewComment"
        placeholder="Your review"
        rows="3"
        class="border rounded px-3 py-2 text-sm resize-none"
        required
      ></textarea>

      <select
        id="reviewRating"
        class="border rounded px-3 py-2 text-sm"
        required
      >
        <option value="">Rating</option>
        <option value="5">5 - Excellent</option>
        <option value="4">4 - Good</option>
        <option value="3">3 - Okay</option>
        <option value="2">2 - Bad</option>
        <option value="1">1 - Terrible</option>
      </select>

      <button type="submit" class="bg-yellow-500 hover:bg-yellow-600 text-white px-4 py-2 rounded text-sm">
        Submit Review
      </button>
    </form>
  </div>
</div>

  <!-- Zipcode Modal Backdrop -->
  <div id="zipcodeModalBackdrop" class="hidden fixed inset-0 bg-black/40 z-40"></div>

  <!-- Zipcode Modal -->
  <div id="zipcodeModal" class="hidden fixed inset-0 z-50 flex items-center justify-center">
    <div class="bg-white rounded-lg w-full max-w-sm shadow-lg p-6 relative flex flex-col">
      <button id="closeZipcodeModal" class="absolute top-3 right-4 text-gray-400 hover:text-black text-xl">&times;</button>
      <h2 class="text-lg font-semibold mb-4 text-center">Enter Your Zipcode</h2>
      <form id="zipcodeForm" class="flex gap-2">
        <input id="zipcodeInput" type="text" placeholder="e.g. 00920" class="flex-1 border rounded px-3 py-2 text-sm" required pattern="\d{5}" />
        <button type="submit" class="bg-yellow-500 text-white px-4 py-2 rounded text-sm hover:bg-yellow-600">Apply</button>
      </form>
    </div>
  </div>

<!-- Chat Modal -->
<div id="chatModalBackdrop" class="hidden fixed inset-0 bg-black/40 z-40"></div>

<div id="chatModal" class="hidden fixed inset-0 z-50 flex items-center justify-center">
  <div class="bg-white rounded-lg w-full max-w-md max-h-100 shadow-lg p-4 relative flex flex-col h-[80vh]">
    
    <!-- Close button -->
    <button id="closeChatModal" class="absolute top-3 right-4 text-gray-400 hover:text-black text-xl">&times;</button>

    <!-- Chat Header -->
    <div id="chatHeader" class="text-lg font-semibold text-center mb-2">Chat with Business</div>

    <!-- Chat Messages -->
    <div id="chatMessages" class="flex flex-col gap-2 overflow-y-auto p-3 h-full bg-gray-50 rounded">
      <!-- Messages will go here -->
      <div class="text-left self-start bg-gray-200 text-gray-800 px-4 py-2 rounded-lg max-w-xs text-sm shadow">
        Hello! Welcome to our business. How can I help you today?
      </div>
      
    </div>

    <!-- Input -->
    <form id="chatForm" class="flex gap-2 mt-5">
      <input id="chatInput" type="text" placeholder="Type a message..." class="flex-1 border rounded px-3 py-2 text-sm" required />
      <button type="submit" class="bg-blue-600 text-white px-4 py-2 rounded text-sm hover:bg-blue-700">Send</button>
    </form>
  </div>
</div>


